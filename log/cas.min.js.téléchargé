$(function(){function button_popin(){var $button=$("#buttonPopinNumClientMDP"),$buttonClose=$("#popinNumClientMDP .popin-close");$button.on("click",function(){$("#popinNumClientMDP").removeClass("hidden"),$("#popinCache").removeClass("hidden")}),$buttonClose.on("click",function(){$("#popinNumClientMDP").addClass("hidden"),$("#popinCache").addClass("hidden")})}var currentURL=window.location.href,setEnvClass=function(u){var getIndex=function(str){return u.indexOf(str)};return getIndex("canalnet-part-inte1-s1.dev.echonet")!=-1||getIndex("canalnet-pro-inte1-s1.dev.echonet")!=-1||getIndex("canalnet-bpf-inte1-s1.dev.echonet")!=-1?"inte":getIndex("canalnet-part-qual-we1-ap1.bnpparibas.net")!=-1||getIndex("canalnet-pro-qual-we1-ap1.bnpparibas.net")!=-1||getIndex("canalnet-bpf-qual-we1-ap1.bnpparibas.net")!=-1?"qualif":getIndex("prev-m-wev1s1-apv1s1.canalnet-part.bnpparibas.net")!=-1||getIndex("prev-m-wev1s1-apv1s1.canalnet-pro.bnpparibas.net")!=-1||getIndex("prev-m-wev1s1-apv1s1.canalnet-bpf.bnpparibas.net")!=-1?"preview":void 0};$("body").addClass(setEnvClass(currentURL));var isPart=$("body").hasClass("part"),isPro=($("body").hasClass("bpf"),$("body").hasClass("pro")),popinUrl=function(){var result;return isPart&&(result="/rsc/contrib/dev/mode-demo/popin.html"),isPro&&(result="/rsc/contrib/dev/mode-demo/popin-pro.html"),result}(),baseUrl=function(){var goodURL="";switch(ENVIRONNEMENT){case"INTE":goodURL="https://canalnet-part-inte1-s1.dev.echonet";break;case"QUALIF":goodURL="https://canalnet-part-qual-we1-ap1.bnpparibas.net/";break;case"PREVIEW":goodURL="https://prev-m-wev1s1-apv1s1.canalnet-part.bnpparibas.net/";break;case"PROD":goodURL="https://mabanque.bnpparibas/"}return goodURL}(),targetURLPopin=baseUrl+popinUrl,initElements={btOpenAccount:function(){$(".bouton-devenir-client").on("mouseenter",function(){is_hover=!0,$(".wrapper-popin-demo").length?$(".wrapper-popin-demo").removeClass("hidden"):$.ajax({type:"GET",url:targetURLPopin,contentType:"html"}).done(function(da