dden")):$(".sosB3").hasClass("active")&&(contentRightSelect.find(".changeContent").addClass("hidden"),contentRightSelect.find("#sosB3").removeClass("hidden"))})}},eltToWait="header.top-header",waitForEl=function(selector,callback){$(selector).length?callback():setTimeout(function(){waitForEl(selector,callback)},100)};waitForEl(eltToWait,function(){initElements.btSos()});var eltToWait2="#buttonPopinNumClientMDP";waitForEl(eltToWait2,function(){button_popin()})});/* lastmodified = 2021-09-27T10:43:07.497Z*/
